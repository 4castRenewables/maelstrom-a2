#!/bin/bash -x
#SBATCH --nodes=#NODES#
#SBATCH --time=#WALLTIME#
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=8
#SBATCH -A #ACCOUNT#
#SBATCH --partition=#PARTITION#
#SBATCH --gres=gpu:1
